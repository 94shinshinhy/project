{{>layouts/header}}

<table class="table">
    <thead>
    <tr>
        <th scope="col">NO</th>
        <th scope="col">제목</th>
        <th scope="col">내용</th>
    </tr>
    </thead>
    {{#article}}
    <tbody>
    <tr>
        <th>{{id}}</th>
        <td>{{title}}</td>
        <td>{{content}}</td>
    </tr>
    </tbody>
    {{/article}}
</table>

<a href="/articles/{{article.id}}/edit" class="btn btn-primary">수정</a>
<a href="/articles/{{article.id}}/delete" class="btn btn-danger">삭제</a>
<a href="/articles" class="btn btn-primary">목록</a>

{{>layouts/footer}}